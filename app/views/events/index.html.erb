<h1><%= @title %></h1>

<div class="row">
  <div class="search_form">
    <%= render 'events/search_form' %>
  </div>
</div>

<%= will_paginate %>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Event Date</th>
      <th scope="col">Event Name</th>
      <th scope="col">Event Source</th>
      <th scope="col">Event URL</th>
    </tr>
  </thead>

  <tbody>
    <%= will_paginate %>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.date %></td>
        <td><%= event.name %></td>
        <td><%= event.source %></td>
        <td><%= link_to("URL", event.url) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= will_paginate %>
